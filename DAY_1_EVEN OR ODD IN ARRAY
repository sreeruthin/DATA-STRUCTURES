#include<stdio.h>
int main() {
    int n;
    printf("192324112-sree ruthin reddy");
    printf("Enter the size of array: ");
    scanf("%d",&n);
    int arr[n];
    printf("\nEnter the array elements: ");
    for(int i=0;i<n;i++) {
        scanf("%d",&arr[i]);
    }
    printf("\nEven numbers in the array: ");
    for(int i=0;i<n;i++) {
        if(arr[i] % 2 == 0) {
            printf("%d ", arr[i]);
        }
    }
    printf("\nOdd numbers in the array: ");
    for(int i=0;i<n;i++) {
        if(arr[i] % 2 != 0) {
            printf("%d ", arr[i]);
        }
    }
    return 0;
}
