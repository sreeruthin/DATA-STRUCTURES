#include<stdio.h>
int main() {
    int n;
    printf("192324112-sree ruthin reddy\n");
    printf("Enter the size of array: ");
    scanf("%d",&n);
    int arr[n];
    printf("\nEnter the array elements: ");
    for(int i=0;i<n;i++) {
        scanf("%d",&arr[i]);
    }

    printf("\nDuplicate elements in the array: ");
    for(int i=0;i<n;i++) {
        for(int j=i+1;j<n;j++) {
            if(arr[i] == arr[j]) {
                printf("%d ", arr[i]);
            }
        }
    }

    return 0;
}
